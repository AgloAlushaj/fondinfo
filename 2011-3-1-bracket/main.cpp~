#include <iostream>
#include <fstream>

using namespace std;

int main(int argc, char *argv[])
{

    ifstream file("../2011-2-6-dash/main.cpp");
    file >> noskipws;

    bool dash = false;

    char c;
    file >> c;
    while (file.good()) {

        if ('a' <= c && c <= 'z') {

            dash = false;
        } else if (c == ')' && inside) {
            inside = false;
        } else if (!inside) {
            cout << c;
        }

        file >> c;
    }

    return 0;
}
